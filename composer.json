{
    "name": "rookmoot/metascrapper",
    "description": "Laravel package to get all the meta data from a URL",
    "license": "unlicense",
    "authors": [
        {
            "name": "Hossein Jabbari",
            "email": "mr.hhkj@gmail.com",
            "homepage": "https://github.com/hojabbr"
        }
    ],
    "homepage": "https://github.com/rookmoot/metascrapper",
    "keywords": [
        "Laravel",
        "MetaScrapper"
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "rookmoot\\MetaScrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rookmoot\\MetaScrapper\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rookmoot\\MetaScrapper\\MetaScrapperServiceProvider"
            ],
            "aliases": {
                "MetaScrapper": "rookmoot\\MetaScrapper\\Facades\\MetaScrapper"
            }
        }
    }
}
