{
    "name": "awizemann/metascraper",
    "description": "Laravel package to get all the meta data from a URL (forked from rookmoot/metascraper)",
    "license": "MIT",
    "authors": [
        {
            "name": "Alan Wizemann",
            "email": "awizemann+meta@gmail.com",
            "homepage": "https://github.com/awizemann"
        }
    ],
    "homepage": "https://github.com/awizemann/metascraper",
    "keywords": [
        "Laravel",
        "Meta Tags"
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4.2"
    },
    "autoload": {
        "psr-4": {
            "awizemann\\metascraper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "awizemann\\metascraper\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "awizemann\\metascraper\\meta-scraperServiceProvider"
            ],
            "aliases": {
                "metascraper": "awizemann\\metascraper\\Facades\\metascraper"
            }
        }
    }
}
